{
    "name": "tomatophp/tomato-eddy",
    "type": "library",
    "description": "Eddy Server Management integration for TomatoPHP",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoEddy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoEddy\\TomatoEddyServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "protonemedia/laravel-task-runner": "^1.0",
        "spatie/laravel-csp": "^2.8",
        "maatwebsite/excel": "^3.1",
        "kirschbaum-development/eloquent-power-joins": "^3.2",
        "jamesryanbell/cloudflare": "^1.11",
        "bepsvpt/secure-headers": "^7.4",
        "moneyphp/money": "^4.3",
        "phpseclib/phpseclib": "~3.0",
        "laravel/socialite": "^5.11",
        "pusher/pusher-php-server": "^7.2"
    }
}
